@font-face {
  font-family: 'test-icon';
  src:  url('../fonts/test-icon.eot?rxtdec');
  src:  url('../fonts/test-icon.eot?rxtdec#iefix') format('embedded-opentype'),
    url('../fonts/test-icon.ttf?rxtdec') format('truetype'),
    url('../fonts/test-icon.woff?rxtdec') format('woff'),
    url('../fonts/test-icon.svg?rxtdec#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'test-icon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

